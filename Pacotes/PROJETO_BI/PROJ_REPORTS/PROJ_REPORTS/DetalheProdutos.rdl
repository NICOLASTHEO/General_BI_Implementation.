  <DataSets>
    <DataSet Name="SET_PRODUTOS">
      <Query>
        <DataSourceName>DS_PRODUTOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FORNECEDOR">
            <Value>=Parameters!FORNECEDOR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT P.NOME AS PRODUTO,
C.NOME AS CATEGORIA
FROM DIM_PRODUTO P
INNER JOIN FATO FA
ON P.IDSK = FA.IDPRODUTO
INNER JOIN DIM_FORNECEDOR F
ON F.IDSK = FA.IDFORNECEDOR
INNER JOIN CATEGORIA C 
ON IDCATEGORIA = IDCATEGORIA
WHERE F.NOME = @FORNECEDOR</CommandText>
      </Query>
      <Fields>
        <Field Name="PRODUTO">
          <DataField>PRODUTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CATEGORIA">
          <DataField>CATEGORIA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
